#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/svelte-check@2.4.6_svelte@3.46.4/node_modules/svelte-check/bin/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/svelte-check@2.4.6_svelte@3.46.4/node_modules/svelte-check/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/svelte-check@2.4.6_svelte@3.46.4/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules:/Users/michele/Progetti/node_modules:/Users/michele/node_modules:/Users/node_modules:/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/svelte-check/bin/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/svelte-check/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/svelte-check@2.4.6_svelte@3.46.4/node_modules/svelte-check/bin/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/svelte-check@2.4.6_svelte@3.46.4/node_modules/svelte-check/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/svelte-check@2.4.6_svelte@3.46.4/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules:/Users/michele/Progetti/node_modules:/Users/michele/node_modules:/Users/node_modules:/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/svelte-check/bin/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/svelte-check/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
