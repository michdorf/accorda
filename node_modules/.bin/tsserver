#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/typescript@4.6.2/node_modules/typescript/bin/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/typescript@4.6.2/node_modules/typescript/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/typescript@4.6.2/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules:/Users/michele/Progetti/node_modules:/Users/michele/node_modules:/Users/node_modules:/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/typescript/bin/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/typescript/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/typescript@4.6.2/node_modules/typescript/bin/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/typescript@4.6.2/node_modules/typescript/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/typescript@4.6.2/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules:/Users/michele/Progetti/node_modules:/Users/michele/node_modules:/Users/node_modules:/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/typescript/bin/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/typescript/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
