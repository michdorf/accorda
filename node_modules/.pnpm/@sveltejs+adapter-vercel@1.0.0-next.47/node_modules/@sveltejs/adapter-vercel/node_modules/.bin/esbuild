#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/esbuild@0.14.27/node_modules/esbuild/bin/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/esbuild@0.14.27/node_modules/esbuild/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/esbuild@0.14.27/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules:/Users/michele/Progetti/node_modules:/Users/michele/node_modules:/Users/node_modules:/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/@sveltejs+adapter-vercel@1.0.0-next.47/node_modules/@sveltejs/adapter-vercel/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/@sveltejs+adapter-vercel@1.0.0-next.47/node_modules/@sveltejs/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/@sveltejs+adapter-vercel@1.0.0-next.47/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/esbuild@0.14.27/node_modules/esbuild/bin/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/esbuild@0.14.27/node_modules/esbuild/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/esbuild@0.14.27/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules:/Users/michele/Progetti/node_modules:/Users/michele/node_modules:/Users/node_modules:/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/@sveltejs+adapter-vercel@1.0.0-next.47/node_modules/@sveltejs/adapter-vercel/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/@sveltejs+adapter-vercel@1.0.0-next.47/node_modules/@sveltejs/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/@sveltejs+adapter-vercel@1.0.0-next.47/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../esbuild@0.14.27/node_modules/esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../../../../../../esbuild@0.14.27/node_modules/esbuild/bin/esbuild" "$@"
fi
