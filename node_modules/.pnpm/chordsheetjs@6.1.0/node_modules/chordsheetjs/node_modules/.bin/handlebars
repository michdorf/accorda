#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/handlebars@4.7.7/node_modules/handlebars/bin/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/handlebars@4.7.7/node_modules/handlebars/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/handlebars@4.7.7/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules:/Users/michele/Progetti/node_modules:/Users/michele/node_modules:/Users/node_modules:/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/chordsheetjs@6.1.0/node_modules/chordsheetjs/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/chordsheetjs@6.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/handlebars@4.7.7/node_modules/handlebars/bin/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/handlebars@4.7.7/node_modules/handlebars/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/handlebars@4.7.7/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules:/Users/michele/Progetti/node_modules:/Users/michele/node_modules:/Users/node_modules:/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/chordsheetjs@6.1.0/node_modules/chordsheetjs/node_modules:/Users/michele/Progetti/ChordsViewer/node_modules/.pnpm/chordsheetjs@6.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../handlebars@4.7.7/node_modules/handlebars/bin/handlebars" "$@"
else
  exec node  "$basedir/../../../../../handlebars@4.7.7/node_modules/handlebars/bin/handlebars" "$@"
fi
